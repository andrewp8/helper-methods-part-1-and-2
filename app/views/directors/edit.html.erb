<%= @director.errors.full_messages.each do |msg| %>
  msg
<% end %>
<%= form_with(model: @director, method: :patch, data:{ turbo: false }) do |form| %>
  <div>
    <%= form.label :name %>
  </div>
  <div>
    <%= form.text_field :name%>
  </div>
  <div>
    <%= form.label :dob %>
  </div>
  <div>
    <%= form.date_field :dob %>
  </div>
  <%= form.submit %>
<% end %>
